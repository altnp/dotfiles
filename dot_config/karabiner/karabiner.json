{
    "global": { "show_in_menu_bar": false },
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "description": "Enable delete in Finder",
                        "manipulators": [
                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": ["^com\\.apple\\.finder$"],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "delete_forward" },
                                "to": [
                                    {
                                        "key_code": "delete_or_backspace",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Swap right/left/backspace Cmd and Opt",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": { "key_code": "left_arrow", "modifiers": { "mandatory": ["left_command"] } },
                                "to": [{ "key_code": "left_arrow", "modifiers": ["left_option"] }]
                            },
                            {
                                "type": "basic",
                                "from": { "key_code": "right_arrow", "modifiers": { "mandatory": ["left_command"] } },
                                "to": [{ "key_code": "right_arrow", "modifiers": ["left_option"] }]
                            },
                            {
                                "type": "basic",
                                "from": { "key_code": "left_arrow", "modifiers": { "mandatory": ["left_option"] } },
                                "to": [{ "key_code": "left_arrow", "modifiers": ["left_command"] }]
                            },
                            {
                                "type": "basic",
                                "from": { "key_code": "right_arrow", "modifiers": { "mandatory": ["left_option"] } },
                                "to": [{ "key_code": "right_arrow", "modifiers": ["left_command"] }]
                            },
                            {
                                "type": "basic",
                                "from": { "key_code": "delete_or_backspace", "modifiers": { "mandatory": ["left_command"] } },
                                "to": [{ "key_code": "delete_or_backspace", "modifiers": ["left_option"] }]
                            },
                            {
                                "type": "basic",
                                "from": { "key_code": "delete_or_backspace", "modifiers": { "mandatory": ["left_option"] } },
                                "to": [{ "key_code": "delete_or_backspace", "modifiers": ["left_command"] }]
                            }
                        ]
                    },
                    {
                        "description": "Open ColorSlurp and start picking",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "p",
                                    "modifiers": { "mandatory": ["command", "control"] }
                                },
                                "to": [
                                    {
                                        "shell_command": "/bin/bash -c 'if ! pgrep -x \"ColorSlurp\" >/dev/null; then open -a \"ColorSlurp\"; sleep 0.35; fi; osascript -e \"tell application \\\"System Events\\\" to key code 35 using {command down, control down}\"'"
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "description": "Open Snipping Tool",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "s",
                                    "modifiers": { "mandatory": ["command", "control"] }
                                },
                                "to": [{ "key_code": "4", "modifiers": ["command", "shift"] }]
                            }
                        ]
                    },
                    {
                        "description": "Open Advanced Snipping Tool",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "s",
                                    "modifiers": { "mandatory": ["shift", "command", "control"] }
                                },
                                "to": [{ "key_code": "5", "modifiers": ["command", "shift"] }]
                            }
                        ]
                    }
                ]
            },
            "devices": [
                {
                    "identifiers": {
                        "is_keyboard": true,
                        "product_id": 20616,
                        "vendor_id": 12815
                    },
                    "simple_modifications": [
                        {
                            "from": { "key_code": "right_option" },
                            "to": [{ "apple_vendor_top_case_key_code": "keyboard_fn" }]
                        },
                        {
                            "from": { "key_code": "left_command" },
                            "to": [{ "key_code": "left_option" }]
                        },
                        {
                            "from": { "key_code": "left_option" },
                            "to": [{ "key_code": "left_command" }]
                        }
                    ]
                }
            ],
            "name": "Default profile",
            "selected": true,
            "virtual_hid_keyboard": { "keyboard_type_v2": "ansi" }
        }
    ]
}
