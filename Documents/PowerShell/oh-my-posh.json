{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "-> ",
    "foreground": "white",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "\ueab6 ",
    "foreground": "white",
    "background": "transparent"
  },
  "console_title_template": "{{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u256d\u2500",
          "foreground": "blue",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if .Error }}lightRed{{ end }}"]
        },
        {
          "properties": {
            "cache_duration": "none",
            "ubuntu": "<yellow>\uf31b</>",
            "windows": "\ue62a",
            "arch": "<#0f94d2>\udb82\udcc7</>"
          },
          "leading_diamond": "\ue0be",
          "trailing_diamond": "\ue0b8",
          "template": " {{.Icon}} ",
          "foreground": "white",
          "background": "#313131",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "leading_diamond": "\ue0be",
          "trailing_diamond": "\ue0b8",
          "template": " \uf07b {{ .Path }} ",
          "foreground": "white",
          "background": "blue",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "leading_diamond": "\ue0be",
          "trailing_diamond": "\ue0b8",
          "template": " {{ if and .Ahead .Behind }}<yellow>{{ .HEAD }}{{ .BranchStatus }}</>{{ else if .Ahead }}<lightGreen>{{ .HEAD }}{{ .BranchStatus }}</>{{ else if .Behind }}<lightRed>{{ .HEAD }}{{ .BranchStatus }}</>{{ else }}{{ .HEAD }}{{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }}<lightGreen>{{ if gt .Staging.Added 0 }} +{{ .Staging.Added }}{{ end }}{{ if gt .Staging.Modified 0 }} ~{{ .Staging.Modified }}{{ end }}{{ if gt .Staging.Deleted 0 }} -{{ .Staging.Deleted }}{{ end }}</>{{ if gt .Staging.Unmerged 0 }}<lightRed> !{{ .Staging.Unmerged }}</>{{ end }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }}<yellow>{{ if gt .Working.Untracked 0 }} +{{ .Working.Untracked }}{{ end }}{{ if gt .Working.Modified 0 }} ~{{ .Working.Modified }}{{ end }}{{ if gt .Working.Deleted 0 }} -{{ .Working.Deleted }}{{ end }}</>{{ if gt .Working.Unmerged 0 }}<lightRed> !{{ .Working.Unmerged }}</>{{ end }}{{ end }}{{ if gt .StashCount 0 }} \udb80\udd93{{ .StashCount }}{{ end }} ",
          "foreground": "white",
          "background": "#313131",
          "type": "git",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "display_default": false
          },
          "template": "\ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }}",
          "foreground": "#F29100",
          "type": "aws",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ .Venv }} {{ .Full }} ",
          "foreground": "#3399cc",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Env.LOADED_ENV_PROFILE }} \udb80\udd6c {{ .Env.LOADED_ENV_PROFILE }}{{ end }}",
          "foreground": "#8ee343",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "context_aliases": {
              "docker-desktop": ""
            }
          },
          "template": "{{ .Context }}{{ if and .Namespace (ne .Namespace \"docker-desktop\") }} \udb84\udcfe {{ .Namespace }}{{ end }}",
          "foreground": "#3872e7",
          "type": "kubectl",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "blue",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if .Error }}lightRed{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
