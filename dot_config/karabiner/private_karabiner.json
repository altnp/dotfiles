        {
            "complex_modifications": {
                "rules": [
	                    {
	                        "description": "Enable delete in Finder",
	                        "manipulators": [
	                            {
                                "conditions": [
                                    {
                                        "bundle_identifiers": ["^com\\.apple\\.finder$"],
                                        "type": "frontmost_application_if"
                                    }
                                ],
                                "from": { "key_code": "delete_forward" },
                                "to": [
                                    {
                                        "key_code": "delete_or_backspace",
                                        "modifiers": ["command"]
                                    }
                                ],
                                "type": "basic"
	                            }
	                        ]
	                    },
	                    {
	                        "description": "Open ColorSlurp - Cmd+Shift+2",
	                        "manipulators": [
	                            {
	                                "from": {
	                                    "key_code": "2",
	                                    "modifiers": { "mandatory": ["command", "shift"], "optional": ["any"] }
	                                },
	                                "to": [
	                                    {
	                                        "shell_command": "/bin/bash -c 'if ! pgrep -x \"ColorSlurp\" >/dev/null; then open -a \"ColorSlurp\"; sleep 0.35; fi; osascript -e \"tell application \\\"System Events\\\" to key code 35 using {command down, control down, option down, shift down}\"'"
	                                    }
	                                ],
	                                "type": "basic"
	                            }
	                        ]
	                    },
